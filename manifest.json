{
  "name": "rua豹器",
  "manifest_version": 2,
  "version": "4.10",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "description": "rua!",
  "permissions": ["cookies","storage","notifications","webRequest","webRequestBlocking","https://api.live.bilibili.com/*","https://api.bilibili.com/*", "https://live.bilibili.com/", "https://www.bilibili.com/", "https://*.hdslb.com/*"],
  "background": {
    "scripts": ["js/ADT.js","js/Jquery-3.5.1.js","js/PushHost.js","js/background.js"],
