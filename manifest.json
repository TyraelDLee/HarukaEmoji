{
  "name": "rua豹器",
  "manifest_version": 2,
  "version": "3.2",
  "description": "rua!",
  "permissions": ["cookies", "notifications","https://api.live.bilibili.com/*","https://api.bilibili.com/*", "https://live.bilibili.com/", "https://www.bilibili.com/", "http://i0.hdslb.com/*"],
  "background": {
    "scripts": ["js/background.js","js/Jquery-3.5.1.js"],
    "persistent": true
  },
  "icons": {
    "16": "images/haruka16.png",
    "32": "images/haruka32.png",
    "48": "images/haruka48.png",
    "128": "images/haruka128.png"
  },
  "content_scripts": [ {
    "all_frames": true,
    "css": ["css/host.css"],
    "js": ["js/haruka.js","js/WebHost.js","js/Jquery-3.5.1.js"],
    "matches": [ "*://live.bilibili.com/21652717*", "*://live.bilibili.com/21652717?*", "*://live.bilibili.com/record/21652717*"],
    "run_at": "document_end"
  } ],
  "web_accessible_resources": ["images/*", "css/*", "js/*"],
  "content_security_policy": "script-src 'self' https://api.live.bilibili.com; object-src 'self'"
}
