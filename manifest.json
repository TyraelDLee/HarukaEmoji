  },
  "icons": {
    "16": "images/haruka16.png",
    "32": "images/haruka32.png",
    "48": "images/haruka48.png",
    "128": "images/haruka128.png"
  },
  "content_scripts": [ {
    "all_frames": true,
    "js": ["js/ADT.js","js/Jquery-3.5.1.js","js/Madel.js"],
